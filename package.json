{
  "name": "js-starterkit",
  "version": "1.0.0",
  "description": "Base js starter kit - A check list for all JS projects",
  "scripts": {
    "prestart": "node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src",
    "open:src": "node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roblav/jsStarterkit.git"
  },
  "author": "Robert Lavender",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roblav/jsStarterkit/issues"
  },
  "homepage": "https://github.com/roblav/jsStarterkit#readme",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "casperjs": "^1.1.3",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "express": "4.14.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "nunjucks": "^3.0.0",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.14"
  }
}
